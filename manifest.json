{
  "name": "Smart Bookmark Sorter",
  "version": "0.2.0.7",
  "manifest_version": 2,
  "description": "Sort bookmarks based on their taxonomy",
  "icons": { "128": "images/logo_128x128.png", "48" : "images/logo_48x48.png", "16" : "images/logo_16x16.png"},
  "browser_action": {
    "default_icon": "images/logo_48x48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "bg.html",
    "persistent" : false
  },
  "permissions": [
	"bookmarks",
	"storage",
	"alarms",
	"background",
    "chrome://favicon/",
	"https://access.alchemyapi.com/"
  ]
}